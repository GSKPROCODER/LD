#include<iostream>
using namespace std;

int main() {
    int n;
    int pos;
    int value;

    cout<<"Enter the number of elements in the array: ";
    cin>>n;

    int arr[n+1];

    cout<<"Enter the elements of the array: ";
    for(int i=0; i<n; i++) {
        cin>>arr[i];
    }

    cout<<"Enter position to insert element: ";
    cin>>pos;

    cout<<"Enter value of element: ";
    cin>>value;

    for(int i=n; i>pos; i--) {
        arr[i] = arr[i-1];
    }

    arr[pos] = value;

    cout<<"The new array is: ";
    for(int i=0; i<=n; i++) {
        cout<<arr[i]<<" ";
    }

    return 0;
}